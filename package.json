{
  "name": "shop-app",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child server/main.ts",
    "db:migrate": "npx prisma migrate dev && yarn prisma generate",
    "db:push": "npx prisma db push",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint server/**/*.ts",
    "format": "eslint server/**/*.ts --fix",
    "eslint:fix": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prisma": {
    "seed": "ts-node-dev --transpile-only prisma/seed.ts"
  },
  "license": "MIT",
  "devDependencies": {
